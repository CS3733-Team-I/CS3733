<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXTextField fx:id="searchField" prefHeight="32.0" prefWidth="300.0" promptText="Search Here">
               <font>
                  <Font size="12.0" />
               </font>
            </JFXTextField>
            <JFXComboBox fx:id="cbNodes" prefHeight="27.0" prefWidth="312.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
